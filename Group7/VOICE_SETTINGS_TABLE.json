  "voice_rotation_strategy": {
    "description": "Prevent listener fatigue by varying voices across the week",
    "patterns": {
      "monday": ["Lyra", "Atlas"],
      "tuesday": ["Nova", "Cipher"],
      "wednesday": ["Echo", "Quantum"],
      "thursday": ["Nexus", "Lyra"],
      "friday": ["Atlas", "Nova"],
      "saturday": ["Cipher", "Echo"],
      "sunday": ["Quantum", "Nexus"]
    },
    "notes": "Rotate 2 agents per day for variety. Adjust based on analytics."
  },

  "global_audio_settings": {
    "output_format": "mp3_44100_128",
    "normalization": {
      "target_lufs": -14,
      "true_peak": -1.0
    },
    "background_music": {
      "enabled": true,
      "volume_db": -22,
      "fade_in": 0.5,
      "fade_out": 1.0
    },
    "noise_reduction": true,
    "de_essing": true
  },

  "elevenlabs_api_config": {
    "endpoint": "https://api.elevenlabs.io/v1/text-to-speech/{voice_id}",
    "headers": {
      "xi-api-key": "{{ELEVENLABS_API_KEY}}",
      "Content-Type": "application/json"
    },
    "timeout": 30000,
    "retry": {
      "max_attempts": 3,
      "backoff": "exponential"
    }
  },

  "usage_notes": [
    "Import this JSON into Make.com Data Store named 'voice_profiles'",
    "Map agent name to voice_id in Module #4 of scenario",
    "Use speed/pitch to create variation within same voice",
    "Monitor voice performance in Notion analytics",
    "Update settings monthly based on engagement metrics",
    "Test new voices in ElevenLabs before adding to production"
  ],

  "voice_testing_checklist": [
    "[ ] Generate 3 sample scripts per agent persona",
    "[ ] Render voice samples with current settings",
    "[ ] A/B test with audience (5+ people)",
    "[ ] Measure: clarity, energy match, emotional resonance",
    "[ ] Adjust stability/style/speed based on feedback",
    "[ ] Document final settings in this JSON",
    "[ ] Deploy to Make.com Data Store"
  ]
