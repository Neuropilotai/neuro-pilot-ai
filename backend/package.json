{
  "name": "neuro-pilot-backend",
  "version": "1.0.0",
  "description": "NEURO.PILOT.AI Backend - Autonomous AI Trading & Resume Generation",
  "main": "server.js",
  "scripts": {
    "start": "node inventory-complete-bilingual.js",
    "start:server": "node server.js",
    "dev": "nodemon inventory-complete-bilingual.js",
    "dev:server": "nodemon server.js",
    "test": "node test_complete_system.js",
    "build-css": "echo 'CSS already built in public/css/inventory.css'",
    "watch-css": "echo 'CSS file is static - edit public/css/inventory.css directly'",
    "monitor": "node super_agent_monitor.js",
    "monitor:dev": "nodemon super_agent_monitor.js",
    "monitor:full": "node start_super_monitor.js",
    "docker:build": "docker build -t neuro-pilot-inventory .",
    "docker:run": "docker run -p 8083:8083 --env-file .env neuro-pilot-inventory",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.yml up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "logs": "tail -f server.log",
    "logs:live": "tail -f server.log inventory.log 2>/dev/null || tail -f server.log",
    "docker:clean": "docker system prune -f",
    "deploy:render": "echo 'Deploying to Render.com...' && curl -X POST https://api.render.com/v1/services/$RENDER_SERVICE_ID/deploys -H 'Authorization: Bearer $RENDER_API_KEY'",
    "health-check": "node -e \"require('http').get('http://localhost:8083/health', (res) => { console.log('Health check:', res.statusCode === 200 ? 'OK' : 'FAILED'); process.exit(res.statusCode === 200 ? 0 : 1) })\""
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "ccxt": "^4.4.90",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "googleapis": "^150.0.1",
    "helmet": "^8.1.0",
    "html-pdf": "^3.0.1",
    "javascript-obfuscator": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "linkedin-api": "^0.0.1",
    "mongoose": "^7.6.3",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "openai": "^5.5.1",
    "puppeteer": "^24.10.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stripe": "^18.2.1",
    "technicalindicators": "^3.1.0",
    "validator": "^13.15.15",
    "websocket-stream": "^5.3.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "tailwindcss": "^4.1.11"
  }
}
