  // ========================================
  // 2. API & MOCK SERVICES
  // ========================================

  // API call with automatic fallback to mock
  apiCall: async function(endpoint, options = {}) {
    const url = `${this.config.apiUrl}${endpoint}`;
    const headers = {
      'Content-Type': 'application/json',
      ...options.headers,
    };

    if (this.config.token && !options.skipAuth) {
      headers['Authorization'] = `Bearer ${this.config.token}`;
    }

    try {
      const response = await fetch(url, {
        ...options,
        headers,
      });

      if (response.status === 401) {
        this.toast('Session expired. Please login again.', 'error');
        this.logout();
        return null;
      }

      if (!response.ok) {
        // If API fails, fallback to mock
        if (this.config.mockMode || response.status === 404) {
          console.warn(`API ${endpoint} failed, using mock data`);
          return this.mockCall(endpoint, options.method || 'GET', options.body ? JSON.parse(options.body) : null);
        }
        throw new Error(`HTTP ${response.status}`);
      }

      return await response.json();
    } catch (error) {
      console.error('API Error:', error);
      // Fallback to mock on network error
      if (this.config.mockMode) {
        console.warn('Using mock data due to network error');
        return this.mockCall(endpoint, options.method || 'GET', options.body ? JSON.parse(options.body) : null);
      }
      this.toast('Network error. Please check your connection.', 'error');
      return null;
    }
  },

  // Mock service implementation
  mockCall: async function(endpoint, method, data) {
    const db = this.mockDB.get();
    const parts = endpoint.split('/').filter(p => p);

    // Auth endpoints
    if (endpoint === '/api/auth/login') {
      const user = db.users.find(u => u.email === data.email && u.password === data.password);
      if (user) {
        const token = 'mock_token_' + Date.now();
        return {
          success: true,
          data: {
            token,
            user: { id: user.id, email: user.email, name: user.name, role: user.role },
          },
        };
      }
      return { success: false, message: 'Invalid credentials' };
    }

    // Items endpoints
    if (endpoint.startsWith('/api/items')) {
      if (method === 'GET' && parts.length === 2) {
        return { success: true, data: db.items };
      }
      if (method === 'POST') {
        const newItem = { id: Date.now(), ...data, active: data.active !== false };
        db.items.push(newItem);
        this.mockDB.set(db);
        this.logAudit('CREATE', 'items', `Created item ${newItem.sku}`);
        return { success: true, data: newItem };
      }
      if (method === 'PUT') {
        const id = parseInt(parts[2]);
        const index = db.items.findIndex(i => i.id === id);
        if (index >= 0) {
          db.items[index] = { ...db.items[index], ...data };
          this.mockDB.set(db);
          this.logAudit('UPDATE', 'items', `Updated item ${db.items[index].sku}`);
          return { success: true, data: db.items[index] };
        }
      }
      if (method === 'DELETE') {
        const id = parseInt(parts[2]);
        db.items = db.items.filter(i => i.id !== id);
        this.mockDB.set(db);
        this.logAudit('DELETE', 'items', `Deleted item ID ${id}`);
        return { success: true };
      }
    }

    // Inventory endpoints
    if (endpoint.startsWith('/api/inventory')) {
      if (method === 'GET') {
        return { success: true, data: db.inventory };
      }
    }

    // Dashboard stats
    if (endpoint === '/api/owner/dashboard/stats') {
      const totalItems = db.items.filter(i => i.active).length;
      const totalQty = db.inventory.reduce((sum, inv) => sum + inv.quantity, 0);
      const lowStock = db.items.filter(item => {
        const inv = db.inventory.find(i => i.itemId === item.id);
        return inv && inv.quantity < (item.par || 0) * 0.3;
      }).length;

      return {
        success: true,
        stats: {
          pdfs: { coverage: 0, withDates: 0, total: 0 },
          inventory: { totalItems, totalQuantity: totalQty, locations: db.locations.length },
          fifo: { totalCases: db.lots.length, productsTracked: db.lots.length },
        },
      };
    }

    // Metrics endpoint
    if (endpoint === '/api/metrics') {
      return `# HELP http_requests_total Total HTTP requests
# TYPE http_requests_total counter
http_requests_total 1234
# HELP http_request_duration_ms HTTP request latency
# TYPE http_request_duration_ms histogram
http_request_duration_ms_bucket{le="50"} 800
http_request_duration_ms_bucket{le="100"} 950
http_request_duration_ms_bucket{le="250"} 990
http_request_duration_ms_bucket{le="+Inf"} 1000
http_request_duration_ms_count 1000
# HELP nodejs_heap_size_used_bytes Heap used
# TYPE nodejs_heap_size_used_bytes gauge
nodejs_heap_size_used_bytes 45678912
`;
    }

    // Default response
    return { success: true, data: [], message: 'Mock endpoint' };
  },

