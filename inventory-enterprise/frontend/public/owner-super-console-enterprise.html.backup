  items: {
    page: 1,
    pageSize: 20,
    sortBy: 'sku',
    sortDir: 'asc',
    filters: { search: '', category: '', active: '' },
  },

  loadItems: async function() {
    const result = await this.apiCall('/api/items');
    if (result && result.success) {
      this.state.items = result.data;
      this.renderItems();
    }
  },

  renderItems: function() {
    let items = [...this.state.items];

    // Apply filters
    if (this.items.filters.search) {
      const search = this.items.filters.search.toLowerCase();
      items = items.filter(i =>
        i.sku.toLowerCase().includes(search) ||
        i.name.toLowerCase().includes(search)
      );
    }
    if (this.items.filters.category) {
      items = items.filter(i => i.category === this.items.filters.category);
    }
    if (this.items.filters.active !== '') {
      items = items.filter(i => i.active === (this.items.filters.active === 'true'));
    }

    // Sort
    items.sort((a, b) => {
      const aVal = a[this.items.sortBy];
      const bVal = b[this.items.sortBy];
      const dir = this.items.sortDir === 'asc' ? 1 : -1;
      return aVal > bVal ? dir : -dir;
    });

    // Paginate
    const start = (this.items.page - 1) * this.items.pageSize;
    const end = start + this.items.pageSize;
    const pageItems = items.slice(start, end);
    const totalPages = Math.ceil(items.length / this.items.pageSize);

    // Render
    const tbody = document.getElementById('itemsTable');
    tbody.innerHTML = pageItems.map(item => `
      <tr class="hover:bg-slate-50 dark:hover:bg-slate-800">
        <td class="px-4 py-3 text-sm font-medium text-slate-900 dark:text-white">${item.sku}</td>
        <td class="px-4 py-3 text-sm text-slate-900 dark:text-white">${item.name}</td>
        <td class="px-4 py-3 text-sm text-slate-600 dark:text-slate-400">${item.category || '-'}</td>
        <td class="px-4 py-3 text-sm text-slate-600 dark:text-slate-400">${item.uom || '-'}</td>
        <td class="px-4 py-3 text-sm text-slate-600 dark:text-slate-400">${item.par || '-'}</td>
        <td class="px-4 py-3">
          <span class="px-2 py-1 text-xs font-semibold rounded-full ${item.active ? 'bg-green-100 text-green-600 dark:bg-green-900 dark:text-green-400' : 'bg-gray-100 text-gray-600'}">
            ${item.active ? 'Active' : 'Inactive'}
          </span>
        </td>
        <td class="px-4 py-3 text-right text-sm">
          <button onclick="APP.editItem(${item.id})" class="text-blue-600 hover:text-blue-800 dark:text-blue-400 mr-3">
            <i class="ti ti-edit"></i>
          </button>
          <button onclick="APP.deleteItem(${item.id})" class="text-red-600 hover:text-red-800 dark:text-red-400">
            <i class="ti ti-trash"></i>
          </button>
        </td>
      </tr>
    `).join('');

    // Update pagination
    document.getElementById('itemsPageInfo').textContent = `Page ${this.items.page} of ${totalPages || 1}`;
    document.getElementById('itemsPrevPage').disabled = this.items.page === 1;
    document.getElementById('itemsNextPage').disabled = this.items.page >= totalPages;

    // Update category filter dropdown
    const categories = [...new Set(this.state.items.map(i => i.category).filter(Boolean))];
    const categoryFilter = document.getElementById('itemCategoryFilter');
    const currentValue = categoryFilter.value;
    categoryFilter.innerHTML = '<option value="">All Categories</option>' +
      categories.map(cat => `<option value="${cat}">${cat}</option>`).join('');
    categoryFilter.value = currentValue;
  },

  addItem: async function(itemData) {
    const result = await this.apiCall('/api/items', {
      method: 'POST',
      body: JSON.stringify(itemData),
    });

    if (result && result.success) {
      this.toast('Item added successfully', 'success');
      this.loadItems();
      this.closeModal('addItemModal');
    } else {
      this.toast('Failed to add item', 'error');
