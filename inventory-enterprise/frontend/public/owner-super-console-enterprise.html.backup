      const db = this.mockDB.get();
      document.getElementById('dashTotalItems').textContent = db.items.length;
      document.getElementById('dashTotalQty').textContent = db.items.reduce((sum, item) => sum + (item.quantity || 0), 0);
      document.getElementById('dashLowStock').textContent = db.items.filter(i => (i.quantity || 0) < (i.par_level || 0)).length;
    }

    // Load API latency from metrics (optional - gracefully degrade if not available)
    // Note: Using /metrics instead of /api/metrics (no auth required for Prometheus)
    const latencyEl = document.getElementById('dashLatency');
    if (latencyEl) {
      try {
        const controller = new AbortController();
        const timeoutId = setTimeout(() => controller.abort(), 2000);

        const response = await fetch(this.config.apiUrl + '/metrics', {
          signal: controller.signal
        });

        clearTimeout(timeoutId);

        if (response.ok) {
          const metrics = await response.text();
          const p95 = this.calculateP95FromPrometheus(metrics);
          latencyEl.innerHTML = `${p95}<span class="text-base">ms</span>`;
        } else {
          latencyEl.innerHTML = `<span class="text-base text-slate-400">N/A</span>`;
        }
      } catch (e) {
        // Silently fail - metrics are optional
        latencyEl.innerHTML = `<span class="text-base text-slate-400">N/A</span>`;
      }
    }

    // Load recent activity
    this.loadRecentActivity();

    // Initialize charts
    this.initDashboardCharts();
