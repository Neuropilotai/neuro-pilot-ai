  // 7. INVENTORY
  // ========================================

  loadInventory: async function() {
    const invResult = await this.apiCall('/api/inventory');
    const db = this.mockDB.get();

    if (invResult && invResult.success) {
      this.state.inventory = invResult.data;
    } else {
      this.state.inventory = db.inventory;
    }

    // Group by location
    const byLocation = {};
    this.state.inventory.forEach(inv => {
      const loc = inv.locationName || 'Unknown';
      if (!byLocation[loc]) byLocation[loc] = { quantity: 0, items: 0 };
      byLocation[loc].quantity += inv.quantity;
      byLocation[loc].items++;
    });

    const locationsList = document.getElementById('locationsList');
    locationsList.innerHTML = Object.entries(byLocation).map(([loc, data]) => `
      <div class="flex justify-between items-center p-3 bg-slate-50 dark:bg-slate-800 rounded-lg">
        <div>
          <p class="font-semibold text-slate-900 dark:text-white">${loc}</p>
          <p class="text-xs text-slate-500 dark:text-slate-400">${data.items} items</p>
        </div>
        <p class="text-lg font-bold text-blue-600 dark:text-blue-400">${data.quantity}</p>
      </div>
    `).join('');

    // Render lots
    const lotsList = document.getElementById('lotsList');
    lotsList.innerHTML = db.lots.map(lot => {
      const item = db.items.find(i => i.id === lot.itemId);
      const daysUntilExpiry = dayjs(lot.expiryDate).diff(dayjs(), 'day');
      const expiryColor = daysUntilExpiry < 7 ? 'text-red-600 dark:text-red-400' : daysUntilExpiry < 30 ? 'text-yellow-600 dark:text-yellow-400' : 'text-green-600 dark:text-green-400';

      return `
        <div class="flex justify-between items-center p-3 bg-slate-50 dark:bg-slate-800 rounded-lg">
          <div class="flex-1">
            <p class="font-semibold text-slate-900 dark:text-white text-sm">${item ? item.name : 'Unknown'}</p>
            <p class="text-xs text-slate-500 dark:text-slate-400">Lot: ${lot.lotNumber} | Qty: ${lot.quantity}</p>
          </div>
          <div class="text-right">
