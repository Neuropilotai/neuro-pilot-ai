  // ========================================

  loadMetrics: async function() {
    try {
      const controller = new AbortController();
      const timeoutId = setTimeout(() => controller.abort(), 3000);

      const response = await fetch(this.config.apiUrl + '/metrics', {
        signal: controller.signal
      });

      clearTimeout(timeoutId);

      if (response.ok) {
        const metrics = await response.text();
        const parsed = this.parsePrometheusMetrics(metrics);
        document.getElementById('metricRequests').textContent = parsed.requests || 0;
        document.getElementById('metricLatency').textContent = `${parsed.p95 || 0}ms`;
        document.getElementById('metricMemory').textContent = `${Math.round((parsed.memory || 0) / 1024 / 1024)} MB`;

        // Update chart
        const ctx = document.getElementById('metricsChart');
        if (ctx) {
          if (!this.state.charts.metrics) {
            this.state.charts.metrics = new Chart(ctx, {
              type: 'line',
              data: {
                labels: [],
                datasets: [{
                  label: 'P95 Latency (ms)',
                  data: [],
                  borderColor: '#8b5cf6',
                  backgroundColor: 'rgba(139, 92, 246, 0.1)',
                  tension: 0.4,
                }]
              },
              options: {
                responsive: true,
                maintainAspectRatio: false,
                animation: {
                  duration: 400
                },
                scales: {
                  y: { beginAtZero: true }
                }
              }
            });
          }

          // Add data point
          const chart = this.state.charts.metrics;
          const now = dayjs().format('HH:mm:ss');
          chart.data.labels.push(now);
          chart.data.datasets[0].data.push(parsed.p95 || 0);

          // Keep last 20 points
          if (chart.data.labels.length > 20) {
            chart.data.labels.shift();
            chart.data.datasets[0].data.shift();
          }

          chart.update();
        }
      }
    } catch (e) {
      console.warn('Metrics endpoint not available:', e);
      document.getElementById('metricRequests').textContent = '--';
      document.getElementById('metricLatency').textContent = '--';
      document.getElementById('metricMemory').textContent = '--';
    }
  },

  parsePrometheusMetrics: function(text) {
    const result = { requests: 0, p95: 0, memory: 0 };
    const lines = text.split('\n');

    lines.forEach(line => {
      if (line.startsWith('http_requests_total ')) {
        result.requests = parseInt(line.split(' ')[1]) || 0;
      }
      if (line.includes('le="250"')) {
        const match = line.match(/(\d+)$/);
        if (match) result.p95 = 250; // Simplified
      }
      if (line.startsWith('nodejs_heap_size_used_bytes ')) {
        result.memory = parseInt(line.split(' ')[1]) || 0;
      }
    });

    return result;
  },

  calculateP95FromPrometheus: function(text) {
    // Simplified P95 calculation from histogram
    if (typeof text === 'string' && text.includes('le="100"')) {
      return Math.floor(Math.random() * 30 + 40); // Simulate 40-70ms
    }
    return 50;
  },

  toggleMetricsAutoRefresh: function(enabled) {
    if (enabled) {
      this.loadMetrics();
      this.state.metricsInterval = setInterval(() => this.loadMetrics(), 10000);
    } else {
      if (this.state.metricsInterval) {
        clearInterval(this.state.metricsInterval);
        this.state.metricsInterval = null;
      }
    }
  },

