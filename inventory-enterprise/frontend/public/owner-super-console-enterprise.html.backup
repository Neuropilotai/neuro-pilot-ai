  editItem: function(id) {
    const item = this.state.items.find(i => i.id === id);
    if (item) {
      // Open modal with item data
      document.getElementById('newItemSku').value = item.sku;
      document.getElementById('newItemName').value = item.name;
      document.getElementById('newItemCategory').value = item.category || '';
      document.getElementById('newItemUom').value = item.uom || '';
      document.getElementById('newItemPar').value = item.par || '';
      document.getElementById('newItemActive').checked = item.active;

      // Store editing ID
      this.state.editingItemId = id;
      document.getElementById('addItemModal').classList.remove('hidden');
