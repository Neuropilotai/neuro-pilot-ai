        category: document.getElementById('newItemCategory').value,
        uom: document.getElementById('newItemUom').value,
        par: parseInt(document.getElementById('newItemPar').value) || 0,
        active: document.getElementById('newItemActive').checked,
      };

      if (this.state.editingItemId) {
        // Update
        await this.apiCall(`/api/items/${this.state.editingItemId}`, {
          method: 'PUT',
          body: JSON.stringify(itemData),
        });
        this.toast('Item updated successfully', 'success');
      } else {
        // Create
        await this.addItem(itemData);
      }
    });

    // Close modals
    document.getElementById('closeAddItemModal').addEventListener('click', () => this.closeModal('addItemModal'));
    document.getElementById('cancelAddItem').addEventListener('click', () => this.closeModal('addItemModal'));
    document.getElementById('closeSettingsModal').addEventListener('click', () => this.closeModal('settingsModal'));
    document.getElementById('cancelSettings').addEventListener('click', () => this.closeModal('settingsModal'));

    // Save settings
    document.getElementById('saveSettings').addEventListener('click', () => this.saveSettings());

    // Clear data
    document.getElementById('clearDataBtn').addEventListener('click', () => this.clearAllData());

    // Export config
    document.getElementById('exportConfigBtn').addEventListener('click', () => this.exportConfig());

    // Items filters
    document.getElementById('itemSearch').addEventListener('input', (e) => {
      this.items.filters.search = e.target.value;
      this.items.page = 1;
      this.renderItems();
    });

    document.getElementById('itemCategoryFilter').addEventListener('change', (e) => {
      this.items.filters.category = e.target.value;
      this.items.page = 1;
      this.renderItems();
    });

    document.getElementById('itemActiveFilter').addEventListener('change', (e) => {
      this.items.filters.active = e.target.value;
      this.items.page = 1;
      this.renderItems();
    });

    // Items pagination
    document.getElementById('itemsPrevPage').addEventListener('click', () => {
      if (this.items.page > 1) {
        this.items.page--;
        this.renderItems();
      }
    });

    document.getElementById('itemsNextPage').addEventListener('click', () => {
      this.items.page++;
      this.renderItems();
    });

    // Items sorting
    document.querySelectorAll('[data-sort]').forEach(th => {
      th.addEventListener('click', () => {
        const field = th.getAttribute('data-sort');
        if (this.items.sortBy === field) {
          this.items.sortDir = this.items.sortDir === 'asc' ? 'desc' : 'asc';
        } else {
          this.items.sortBy = field;
          this.items.sortDir = 'asc';
        }
        this.renderItems();
      });
    });

    // Import/Export buttons
    document.getElementById('importItemsBtn').addEventListener('click', () => this.importCSV('items'));
    document.getElementById('exportItemsBtn').addEventListener('click', () => this.exportCSV('items'));
    document.getElementById('quickImportBtn').addEventListener('click', () => this.importCSV('items'));
    document.getElementById('quickExportBtn').addEventListener('click', () => this.generateCountSheetPDF());
    document.getElementById('exportAuditBtn').addEventListener('click', () => this.exportCSV('audit'));

    // Count sheet button
    const countBtn = document.getElementById('generateCountSheetBtn');
    if (countBtn) {
      countBtn.addEventListener('click', () => this.generateCountSheetPDF());
    }

    // AI Console
    document.getElementById('runAIBtn').addEventListener('click', () => {
      const prompt = document.getElementById('aiPrompt').value;
      if (prompt.trim()) {
        this.runAI(prompt);
      } else {
        this.toast('Please enter a prompt', 'warning');
      }
    });

    // Metrics auto-refresh
    document.getElementById('metricsAutoRefresh').addEventListener('change', (e) => {
      this.toggleMetricsAutoRefresh(e.target.checked);
    });

    // Keyboard shortcuts
    document.addEventListener('keydown', (e) => {
      if (e.key === 'Escape') {
        document.querySelectorAll('.fixed').forEach(modal => {
          if (!modal.classList.contains('hidden') && modal.id !== 'toastContainer') {
            this.closeModal(modal.id);
          }
        });
      }

      // Global shortcuts (g + key)
      if (e.key === 'g') {
        this.state.gPressed = true;
        setTimeout(() => { this.state.gPressed = false; }, 1000);
      }

      if (this.state.gPressed) {
        const shortcuts = {
          'd': 'dashboard',
          'i': 'items',
          'v': 'inventory',
          'm': 'metrics',
          'a': 'ai',
          'p': 'playground',
        };
        if (shortcuts[e.key]) {
          this.navigateTo(shortcuts[e.key]);
        }
      }
    });

    console.log('‚úÖ Enterprise Console Ready');
    console.log('üí° Keyboard shortcuts: g+d (dashboard), g+i (items), g+v (inventory), g+m (metrics), g+a (AI), g+p (playground)');
  }
};

// ========================================
// START APPLICATION
// ========================================
document.addEventListener('DOMContentLoaded', () => {
  APP.init();
});
  </script>

</body>
</html>
