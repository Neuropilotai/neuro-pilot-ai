  login: async function(email, password) {
    const result = await this.apiCall('/api/auth/login', {
      method: 'POST',
      body: JSON.stringify({ email, password }),
      skipAuth: true,
    });

    if (result && result.success) {
      this.config.token = result.data.token;
      this.config.user = result.data.user;
      localStorage.setItem('NP_TOKEN', result.data.token);
      localStorage.setItem('NP_USER', JSON.stringify(result.data.user));

      this.logAudit('LOGIN', 'auth', 'Successful login');
      return true;
    }
    return false;
  },

  logout: function() {
    this.logAudit('LOGOUT', 'auth', 'User logged out');
    localStorage.removeItem('NP_TOKEN');
    localStorage.removeItem('NP_USER');
    this.config.token = null;
    this.config.user = null;
    document.getElementById('loginScreen').classList.remove('hidden');
    document.getElementById('mainApp').classList.add('hidden');
  },

