  importCSV: function(type) {
    const input = document.createElement('input');
    input.type = 'file';
    input.accept = '.csv';
    input.onchange = (e) => {
      const file = e.target.files[0];
      if (file) {
        Papa.parse(file, {
          header: true,
          complete: (results) => {
            this.processCSVImport(type, results.data);
          }
        });
      }
    };
    input.click();
  },

  processCSVImport: async function(type, data) {
    if (type === 'items') {
      const db = this.mockDB.get();
      let imported = 0;

      for (const row of data) {
        if (row.sku && row.name) {
          const item = {
            sku: row.sku,
            name: row.name,
            category: row.category || '',
            uom: row.uom || '',
            par: parseInt(row.par) || 0,
            active: row.active !== 'false',
          };

          await this.apiCall('/api/items', {
            method: 'POST',
            body: JSON.stringify(item),
          });
          imported++;
        }
      }

      this.toast(`Imported ${imported} items`, 'success');
      this.loadItems();
    }
  },

  exportCSV: function(type) {
    let data = [];
    let filename = 'export.csv';

    if (type === 'items') {
      data = this.state.items.map(item => ({
        sku: item.sku,
        name: item.name,
        category: item.category || '',
        uom: item.uom || '',
        par: item.par || '',
        active: item.active,
      }));
      filename = `items_${dayjs().format('YYYY-MM-DD')}.csv`;
    } else if (type === 'audit') {
      const db = this.mockDB.get();
      data = db.audit.map(log => ({
        timestamp: log.timestamp,
        user: log.user,
        action: log.action,
        resource: log.resource,
        details: log.details,
      }));
      filename = `audit_${dayjs().format('YYYY-MM-DD')}.csv`;
    }

    const csv = Papa.unparse(data);
    this.downloadFile(csv, filename, 'text/csv');
  },

