  runPlaygroundPrompt: async function(type) {
    const prompts = {
      order: 'Generate weekly order suggestion based on par levels and current inventory',
      anomaly: 'Detect anomalies in inventory movements over the past 30 days',
      menu: 'Create menu suggestions using on-hand inventory and items expiring soon',
    };

    const prompt = prompts[type];
    document.getElementById('playgroundOutput').classList.remove('hidden');
    document.getElementById('playgroundOutputText').textContent = 'Processing...';

    await new Promise(resolve => setTimeout(resolve, 1500));

    const db = this.mockDB.get();
    let response = '';

    if (type === 'order') {
      const needsReorder = db.items.filter(item => {
        const inv = db.inventory.find(i => i.itemId === item.id);
        return inv && inv.quantity < (item.par || 0) * 0.5;
      });
      response = `Weekly Order Suggestion\n\nItems to reorder (< 50% of par):\n\n`;
      needsReorder.forEach(item => {
        const inv = db.inventory.find(i => i.itemId === item.id);
        const needed = (item.par || 0) - (inv ? inv.quantity : 0);
        response += `- ${item.name} (${item.sku}): Order ${needed} ${item.uom || 'units'}\n`;
      });
    } else if (type === 'anomaly') {
      response = `Anomaly Detection Report\n\nNo significant anomalies detected in inventory movements.\n\nAll changes appear to be within normal parameters.`;
    } else if (type === 'menu') {
      const expiring = db.lots.filter(lot => {
        const days = dayjs(lot.expiryDate).diff(dayjs(), 'day');
        return days < 14 && days >= 0;
      });
      response = `Menu Suggestions (Use Expiring Items)\n\n`;
      expiring.forEach(lot => {
        const item = db.items.find(i => i.id === lot.itemId);
        if (item) {
          response += `- ${item.name}: ${lot.quantity} ${item.uom || 'units'} (expires ${dayjs(lot.expiryDate).format('MMM D')})\n`;
        }
      });
    }

    document.getElementById('playgroundOutputText').textContent = response || 'No results';
    this.logAudit('PLAYGROUND', 'playground', `Ran ${type} prompt`);
  },

  // ========================================
