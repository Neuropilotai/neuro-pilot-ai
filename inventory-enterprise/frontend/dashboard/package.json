{
  "name": "inventory-enterprise-dashboard",
  "version": "3.0.0",
  "description": "Enterprise-grade React dashboard for Inventory Management System with autonomous learning layer, AI tuning, health prediction, security scanning, governance reporting, real-time AI metrics, multi-tenancy, RBAC visualization, E2E testing, performance budgets, and CI/CD deployment",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:strict": "eslint src --ext js,jsx --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:lighthouse": "lhci autorun",
    "test:a11y": "node scripts/a11y-check.js",
    "test:all": "npm run lint:strict && npm run test && npm run test:e2e && npm run test:lighthouse",
    "audit:security": "npm audit --audit-level=high",
    "audit:secrets": "gitleaks detect --source=. --verbose"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.12.0",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^2.2.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.42.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.8.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "lighthouse": "^11.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
