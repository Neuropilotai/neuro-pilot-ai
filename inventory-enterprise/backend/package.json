{
  "name": "inventory-enterprise-v3",
  "version": "23.6.3",
  "description": "Enterprise-grade inventory management system with Phase 16.5 Unified Governance & Predictive UI, real-time command center, 7-day forecasting, WebSocket live updates, Phase 3 Autonomous Learning Layer, AI Tuner with YAML proposals, 24h health prediction, security anomaly detection, automated governance reporting, Generative Intelligence & Autonomous Governance, GPT/Claude-powered executive summaries, self-learning policy adaptation, ISO/SOC compliance scanning, AI Ops automation, predictive incident response, LSTM + Isolation Forest anomaly detection, automated remediation playbooks, multi-channel alerting (Slack, Email, PagerDuty), comprehensive E2E testing, performance monitoring, multi-tenancy, advanced RBAC, real-time AI intelligence, Redis caching, PostgreSQL support, Grafana UI metrics, and CI/CD pipeline",
  "main": "server-v21_1.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "node scripts/inject-version.js",
    "start": "node scripts/init-postgres.js && node server-v21_1.js",
    "start:with-init": "node scripts/init-database.js && node server-v21_1.js",
    "start:postgres": "node scripts/init-postgres.js && node server-v21_1.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "start:all": "AIOPS_ENABLED=true GOVERNANCE_ENABLED=true INSIGHT_ENABLED=true COMPLIANCE_ENABLED=true node server.js",
    "setup": "bash scripts/setup_8083.sh",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:aiops": "mocha test/aiops.test.js --timeout 10000",
    "test:integration": "mocha test/*.test.js --timeout 15000",
    "test:generative": "mocha test/generative-intelligence.test.js --timeout 15000",
    "test:menu": "mocha test/menu.test.js --timeout 10000",
    "test:all": "npm run test:aiops && npm run test:generative && npm run test:integration && npm run test:menu",
    "lint": "eslint .",
    "backup": "node utils/backup.js",
    "migrate": "node migrations/run.js",
    "migrate:reset": "node scripts/reset-migrations.js",
    "migrate:all": "npm run migrate:aiops && npm run migrate:generative",
    "migrate:aiops": "sqlite3 database.db < migrations/004_ai_ops_tables.sql",
    "migrate:generative": "sqlite3 database.db < migrations/005_generative_intelligence_tables.sql",
    "seed:roles": "node scripts/seed_roles_2025-10-07.js",
    "validate:go-live": "bash scripts/validate_go_live_2025-10-07.sh",
    "health": "curl -s http://localhost:8083/health | json_pp",
    "metrics": "curl -s http://localhost:8083/metrics | grep -E '^(governance|insight|compliance|aiops)_'"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.2.1",
    "googleapis": "^144.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pino": "^8.21.0",
    "pino-http": "^9.0.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mocha": "^10.3.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  }
}
